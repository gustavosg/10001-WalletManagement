<Window x:Class="FrontEnd.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="768" Width="1024" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" xmlns:my="http://schemas.microsoft.com/winfx/2006/xaml/presentation/ribbon">
    <Grid>
        <my:Ribbon Name="ribbon1" VerticalAlignment="Top" HorizontalAlignment="Stretch" >
            <my:RibbonTab Header="Telas">
                <my:RibbonGroup Header="Cadastros">
                    <my:RibbonButton Label="Contas" Name="Contas" LargeImageSource="/FrontEnd;component/Images/Account.jpg" Click="Contas_Click" />
                </my:RibbonGroup>
            </my:RibbonTab>

        </my:Ribbon>
        <Grid HorizontalAlignment="Stretch" Margin="0,139,0,0" Name="grid1" VerticalAlignment="Stretch" >
            <Frame HorizontalAlignment="Stretch" Name="frame1" VerticalAlignment="Stretch" />
        </Grid>

    </Grid>
</Window>
